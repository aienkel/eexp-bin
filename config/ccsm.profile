[shift]
s0_initiate_all_edge = TopLeft|TopRight
s0_mode = 1

[scale]
s0_initiate_all_edge = BottomLeft|BottomRight

[addhelper]
s0_window_types = (Toolbar | Utility | Dialog | ModalDialog | Fullscreen | Normal) & !(name=cairo-weather)
s0_brightness = 50
s0_saturation = 80

[animation]
s0_close_effects = animationaddon:Airplane;animation:Fade;animation:Fade;
s0_minimize_effects = animation:Magic Lamp;

[core]
s0_active_plugins = core;composite;opengl;compiztoolbox;decor;regex;move;imgsvg;mousepoll;addhelper;obs;shift;resize;animation;workarounds;wall;vpswitch;imgpng;screenshot;grid;place;unitymtgrabhandles;animationaddon;wobbly;session;expo;scale;ezoom;unityshell;

[unitymtgrabhandles]
s0_show_handles_key = <Alt>x

[unityshell]
s0_icon_size = 38

