[alias]
	ci = commit
	st = !sh -c 'git status|grep \":\"'
	co = checkout
	df = diff
	dflog = log -p
	ls = !sh -c 'git ls-files|ascii2uni -a K'
	lg = log --graph -20 --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %Cgreen(%cr)%Creset %s'

[color]
	diff = always
[user]
	name = eexpress
	email = eexpress@163.com
[http]
	proxy = localhost:8087
	sslVerify = false
[giggle]
	main-window-maximized = false
	main-window-geometry = 876x677+842+226
	file-view-vpane-position = 433
	main-window-view = FileView
	history-view-vpane-position = 402
